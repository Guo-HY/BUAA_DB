 })\r\n          }).then((res) => {\r\n            if (res.data.status == 'success'){\r\n                alert(\"退课成功\"); \r\n                this.$router.push(\"/chooseclass\") // 切换路由\r\n                this.find()\r\n            }\r\n            else{\r\n                alert(\"退课失败,此为必修课!\")\r\n            }\r\n          })\r\n      }\r\n  },\r\n  created() {\r\n    this.find();\r\n  },\r\n  mounted() {}\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"]}]}